<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorator">

    <excludes>
		<pattern>*.action</pattern>
		<pattern>*.jsp</pattern>
		<pattern>*.html</pattern>
		<pattern>*.htm</pattern>
		<pattern>/common/resultdo.jsp</pattern>
		<pattern>/common/json.jsp</pattern>
		<pattern>/common/blank.jsp</pattern>
		<pattern>/common/download.jsp</pattern>

		<pattern>/index.html</pattern>
		<pattern>/error.jsp</pattern>
		<pattern>/download/*</pattern>

    </excludes>

 

	<decorator name="panel" page="/WEB-INF/jsps/template/panel.jsp"/>
	<!-- popup -->
	<decorator name="popup" page="/WEB-INF/jsps/template/popup.jsp"/>
	<!-- normal -->
    <decorator name="main" page="/WEB-INF/jsps/template/main.jsp"/>
    


</decorators>